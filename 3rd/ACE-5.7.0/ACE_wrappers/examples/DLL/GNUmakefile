all: DLL_Today DLL_Newsweek DLL_Main

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.DLL_Today $(@)
	@$(MAKE) -f GNUmakefile.DLL_Newsweek $(@)
	@$(MAKE) -f GNUmakefile.DLL_Main $(@)

.PHONY: DLL_Today
DLL_Today:
	@$(MAKE) -f GNUmakefile.DLL_Today all

.PHONY: DLL_Newsweek
DLL_Newsweek:
	@$(MAKE) -f GNUmakefile.DLL_Newsweek all

.PHONY: DLL_Main
DLL_Main: DLL_Today DLL_Newsweek
	@$(MAKE) -f GNUmakefile.DLL_Main all

