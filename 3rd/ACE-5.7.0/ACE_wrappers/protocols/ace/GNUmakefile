MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: HTBP-target RMCast-target TMCast-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd HTBP && $(MAKE) -f GNUmakefile.HTBP $(@)
	@cd RMCast && $(MAKE) -f GNUmakefile.RMCast $(@)
	@cd TMCast && $(MAKE) -f GNUmakefile.TMCast $(@)

.PHONY: HTBP-target
HTBP-target:
	@cd HTBP && $(MAKE) -f GNUmakefile.HTBP all

.PHONY: RMCast-target
RMCast-target:
	@cd RMCast && $(MAKE) -f GNUmakefile.RMCast all

.PHONY: TMCast-target
TMCast-target:
	@cd TMCast && $(MAKE) -f GNUmakefile.TMCast all

project_name_list:
