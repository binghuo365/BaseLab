else
LDFLAGS      += -L. -L/cygdrive/e/OpenSource/Terabit2_561.4/Terabit/Test_Directory/lib -L$(ACE_ROOT)/lib
endif
CPPFLAGS     += -I$(ACE_ROOT) -I/cygdrive/e/OpenSource/Terabit2_561.4/Terabit/Test_Directory/tests/common
ifeq ($(static_libs),1)
  CPPFLAGS += -DACE_AS_STATIC_LIBS
endif

